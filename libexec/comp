#!/usr/bin/env bash
#
# Summary: Prints completion options for a command
#
# Usage: luav comp <command> [<args>]
#

if [[ $# -ge 1 ]]
then
	command=$1
	shift
else
	usage
fi

if [[ $# -eq 0 ]]
then
	case "$command" in
	get )
		libexec/all
		;;
	set | unset | make )
		libexec/list
		;;
	help )
		libexec/cmds
		;;
	esac
fi
